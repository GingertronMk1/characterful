<h3>Classes</h3>
<table class="table">
    <thead>
    <tr>
        <th>Class</th>
        <th>Subclass</th>
        <th>Level</th>
    </tr>
    </thead>
    <tbody>
    {% for level in levels %}
        <tr>
            <td>{{ level.class }}</td>
            <td>{{ level.subClass }}</td>
            <td>{{ level.level }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

